enum Codes : byte
{
    //Statement of work
    RESPONSE_OK                           = 0x00,
    RESPONSE_ERROR_ON_WORK                = 0x01,
    RESPONSE_NOT_FOUND                    = 0x02,

    //Statement of User
    RESPONSE_BAD_LOGIN                    = 0x03,
    RESPONSE_ALREADY_LOGGED_IN            = 0x04,
    RESPONSE_NOT_LOGGED_IN                = 0x05,
    RESPONSE_LOGGED_IN                    = 0x06,
    RESPONSE_LOGIN_FAILED                 = 0x07,
}